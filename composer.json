{
    "name": "chuoke/umeng-push",
    "description": "友盟推送",
    "keywords": [
        "umeng-push"
    ],
    "homepage": "https://github.com//umeng-push",
    "license": "MIT",
    "authors": [
        {
            "name": "chuoke",
            "email": "chuokechungyoung@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7|^8"
    },
    "require-dev": {
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Chuoke\\UmengPush\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chuoke\\UmengPush\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
